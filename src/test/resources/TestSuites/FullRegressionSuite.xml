<suite name="FullRegressionSuite" verbose="1">
    <!-- Core parameters used for each test -->
    <parameter name="FirstName"  value="Sam"/>
    <parameter name="LastName"  value="Lot"/>
    <parameter name="StreetAddress"  value="990 Paved Rd"/>
    <parameter name="City"  value="Townsville"/>
    <parameter name="State"  value="CN"/>
    <parameter name="Zip"  value="99883"/>
    <parameter name="PhoneNumber"  value="2223334444"/>
    <parameter name="Ssn"  value="999223333"/>
    <parameter name="Username"  value="slot"/>
    <parameter name="Password"  value="Password123"/>
    <parameter name="Confirmation"  value="Password123"/>

    <test name="Positive Registration Test">
        <classes>
            <class name="com.ParaBankSeleniumTesting.tests.register.RegisterTests">
                <methods>
                    <include name="registrationPositiveTest"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="No First Name - Registration Negative Test">
        <parameter name="FirstName"  value=""/>
        <parameter name="ErrorType" value="customer.firstName"/>
        <parameter name="ErrorMsg" value="First name is required."/>
        <classes>
            <class name="com.ParaBankSeleniumTesting.tests.register.RegisterTests">
                <methods>
                    <include name="registrationNegativeTest"/>
                </methods>
            </class>
        </classes>
    </test>

